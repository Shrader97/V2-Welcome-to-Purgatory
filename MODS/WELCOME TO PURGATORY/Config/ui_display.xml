<configs>

<!--remove xpath="/ui_display_info/crafting_category_display/crafting_category_list[@display_type='workbench']/crafting_category[@name='Mods']" /-->

    <append xpath="/ui_display_info/crafting_category_display">

        <crafting_category_list display_type="AmmoBench">
            <crafting_category name="Small" icon="ui_game_symbol_small_caliber" display_name="smallcaliber" />
			<crafting_category name="Med" icon="ui_game_symbol_med_caliber" display_name="medcaliber" />
			<crafting_category name="Large" icon="ui_game_symbol_large_caliber" display_name="large_caliber" />
            <crafting_category name="Shotgun" icon="ui_game_symbol_shotgun_002" display_name="12g" />
            <crafting_category name="Grenade" icon="ui_game_symbol_grenade_002" display_name="Grenade's" />
        </crafting_category_list>

        <crafting_category_list display_type="BenchtopLathe">
            <crafting_category name="Barrel" icon="ui_game_symbol_barrel" display_name="Barrel's" />
			<crafting_category name="Miscellaneous" icon="ui_game_symbol_spring" display_name="Miscellaneous" />
			<crafting_category name="Chamber" icon="ui_game_symbol_chamber" display_name="Chamber" />
        </crafting_category_list>

        <crafting_category_list display_type="GunsmithBence">
            <crafting_category name="Guns" icon="ui_game_symbol_gunsmithing" display_name="Guns" />
            <crafting_category name="Swords" icon="ui_game_symbol_sword" display_name="Swords" />
            <crafting_category name="Ammo/Weapons" icon="ui_game_symbol_knife" display_name="lblCategoryAmmoWeapons" />
        </crafting_category_list>

		<crafting_category_list display_type="workingMicrowave">
			<crafting_category name="Food/Cooking" icon="ui_game_symbol_fork" display_name="lblCategoryFood" />
			<crafting_category name="Science" icon="ui_game_symbol_science" display_name="lblCategoryScience" />
			<crafting_category name="Resources" icon="ui_game_symbol_resource" display_name="lblCategoryResources" />
			<crafting_category name="Canned" icon="ui_game_symbol_canned" display_name="Canned Food" />			
		</crafting_category_list>

		<crafting_category_list display_type="WorkingStove">
			<crafting_category name="Food/Cooking" icon="ui_game_symbol_fork" display_name="lblCategoryFood" />
			<crafting_category name="CFDrink/Cooking" icon="ui_game_symbol_thirst" display_name="lblCategoryDrink" />
			<crafting_category name="Canned" icon="ui_game_symbol_canned" display_name="Canned Food" />		
			<crafting_category name="Science" icon="ui_game_symbol_science" display_name="lblCategoryScience" />	
		</crafting_category_list>

		<crafting_category_list display_type="Recycler">
			<crafting_category name="Resources" icon="ui_game_symbol_resource" display_name="lblCategoryResources" />
		</crafting_category_list>














    </append>


	<append xpath="/ui_display_info/crafting_category_display/crafting_category_list[@display_type='chemistryStation']">
		<crafting_category name="Food/Cooking" icon="ui_game_symbol_fork" display_name="lblCategoryFoodCooking" />
	</append>

	<append xpath="/ui_display_info/crafting_category_display/crafting_category_list[@display_type='campfire']">
		<crafting_category name="Canned" icon="ui_game_symbol_canned" display_name="Canned Food" />
	</append>


	<append xpath="/ui_display_info/item_display">

		<item_display_info display_type="EnergyTabletDisplay" display_group="groupConsumables">
			<display_entry name="dReloadSpeedMultiplier" title_key="xuiSkillReloadSpeed" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
			<display_entry name="dRunSpeed" title_key="statRunSpeed" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>

		<item_display_info display_type="SnowberryGinDisplay" display_group="groupConsumables">
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dBlockDamage" title_key="xuiSkillBlockDamage" display_type="Percent"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>

		<item_display_info display_type="VodkaDisplay" display_group="groupConsumables">
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent" display_leading_plus="true"/>
      <display_entry name="dBartering" title_key="statBarteringSellBuy" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dStunResist" title_key="statStunResist" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>

		<item_display_info display_type="HungerDrinkDisplay" display_group="groupConsumables">
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
		</item_display_info>

		<item_display_info display_type="SnowberryCandyDisplay" display_group="groupConsumables">
			<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
			<display_entry name="dEfficientDigestion" title_key="statEfficientDigestion" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>

		<item_display_info display_type="WaterHealthDisplay" display_group="groupConsumables">
			<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
		</item_display_info>

		<item_display_info display_type="IceDrinkDisplay" display_group="groupConsumables">
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
			<display_entry name="dHyperthermalResist" title_key="statHyperthermalResist" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
		</item_display_info>

	</append>








</configs>
