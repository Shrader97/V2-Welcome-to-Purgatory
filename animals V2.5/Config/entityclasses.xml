        <entity_class name="animalShark" extends="animalTemplateHostile">
            <property name="Class" value="EntityVulture"/>
            <property name="SizeScale" value="0.75"/>
            <property name="PhysicsBody" value="Shark"/>
            <property name="Tags" value="entity,animal,hostile,Shark,perkAT03"/> <!-- Credit for icon https://www.flaticon.com/authors/bzzrincantation -->
            <property name="AvatarController" value="AvatarAnimalController"/>	
            <property name="ModelType" value="Standard"/>
            <property name="HasRagdoll" value="false"/>
            <property name="HasDeathAnim" value="true"/>
            <property name="RootMotion" value="false"/>
            <property name="RagdollOnDeathChance" value="0"/>
            <property name="Mass" value="200"/>
            <property name="Mesh" value="#@modfolder:Resources/sharking.unity3d?Shark001.prefab"/>
            <property name="LookAtAngle" value="0"/>
            <property name="CanBigHead" value="true" />
            <property name="CanClimbLadders" value="true"/>		
            <property name="CanOpenDoors" value="false"/>
            <property name="UserSpawnType" value="Menu"/>	
            
            <property name="MoveSpeed" value="1"/>
            <property name="MoveSpeedAggro" value="1.1, 1.3"/>

            <property name="SightRange" value="70"/>	

            <property name="AITask-1" value="BreakBlock"/>
            <property name="AITask-2" value="DestroyArea"/>
            <property name="AITask-3" value="Territorial"/>
            <property name="AITask-4" value="ApproachAndAttackTarget" data="class=EntityPlayer,-25,EntityBandit,-25,EntityZombie,-30"/>
            <property name="AITask-5" value="ApproachSpot"/>
            <property name="AITask-6" value="Look"/>
            <property name="AITask-7" value="Wander"/>
            <property name="AITask-8" value=""/>
            <property name="AITarget-1" value="SetAsTargetIfHurt"/>
            <property name="AITarget-2" value="BlockingTargetTask"/>
            <property name="AITarget-3" value="SetNearestEntityAsTarget" data="class=EntityPlayer,13,9,EntityBandit,0,-6,EntityZombie,0,-6"/>
            <property name="AITarget-4" value=""/>

            <property name="SoundRandom" value="SharkRoam"/>
            <property name="SoundAlert" value="SharkAlert"/>
            <property name="SoundHurt" value="silencefiller"/>
            <property name="SoundDeath" value="SharkDeath"/>
            <property name="SoundAttack" value="SharkAttack"/>
            <property name="SoundSense" value="SharkSense"/>
            <property name="SoundGiveUp" value="SharkGiveup"/>

            <!-- Gameplay -->
            <property name="ExperienceGain" value="750"/> <!-- XP strong -->
            <property name="DeadBodyHitPoints" value="600"/>
            <property name="HandItem" value="meleeHandAnimalShark"/>
            <property name="LootDropProb" value=".05"/>
            <property name="LootDropEntityClass" value="EntityLootContainerShark"/>

            <drop event="Harvest" name="foodRawMeat" count="0" tool_category="Butcher"/>
            <drop event="Harvest" name="foodRawMeat" tag="butcherHarvest" count="30"/> <!-- animalShark -->
            <drop event="Harvest" name="resourceLeather" tag="butcherHarvest" count="15"/>
            <drop event="Harvest" name="resourceAnimalFat" tag="butcherHarvest" count="5"/>
            <drop event="Harvest" name="resourceBone" tag="butcherHarvest" count="25"/>
            <drop event="Harvest" name="resourceBone" tag="allToolsHarvest" count="5"/>
            <drop event="Harvest" name="foodRawMeat" count="1" tag="WasteTreasuresCompleteHarvest"/>
            <drop event="Harvest" name="resourceLeather" count="1" tag="WasteTreasuresCompleteHarvest"/>
            <drop event="Harvest" name="resourceBone" count="1" tag="WasteTreasuresCompleteHarvest"/>

            <effect_group name="Base Effects">
                <passive_effect name="HealthMax" operation="base_set" value="500"/>
                <passive_effect name="BuffResistance" operation="base_set" value="100" tags="buffIsOnFire,buffBurningFlamingArrow,buffBurningMolotov,buffBurningElement"/>
            </effect_group>

        </entity_class>

